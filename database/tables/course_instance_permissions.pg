columns
    course_instance_id: bigint not null
    course_instance_role: enum_course_instance_role
    id: bigint not null default nextval('course_instance_permissions_id_seq'::regclass)
    user_id: bigint not null

indexes
    course_instance_permissions_pkey: PRIMARY KEY (id) USING btree (id)
    course_instance_permissions_user_id_course_instance_id_key: UNIQUE (user_id, course_instance_id) USING btree (user_id, course_instance_id)
    course_instance_permissions_course_instance_id_idx: USING btree (course_instance_id)

foreign-key constraints
    course_instance_permissions_course_instance_id_fkey: FOREIGN KEY (course_instance_id) REFERENCES pl_courses(id) ON UPDATE CASCADE ON DELETE CASCADE
    course_instance_permissions_user_id_fkey: FOREIGN KEY (user_id) REFERENCES users(user_id) ON UPDATE CASCADE ON DELETE CASCADE
