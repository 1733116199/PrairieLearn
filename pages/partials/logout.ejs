<% if (typeof authn_user != 'undefined') { %>
   <% if (authn_user.provider == 'shibboleth') { %>
       <a class="btn btn-default navbar-btn" href="<%= plainUrlPrefix %>/logout">Log out</a>
   <% } else if (authn_user.provider == 'azure') { %>
       <a class="btn btn-default navbar-btn" href="<%= config.azureDestroySessionUrl %>">Log out</a>
   <% } else { %>
       <a class="btn btn-default navbar-btn" disabled>Log out</a>
   <% } %>
<% } else { %>
   <a class="btn btn-default navbar-btn" disabled>Log out</a>
<% } %>
