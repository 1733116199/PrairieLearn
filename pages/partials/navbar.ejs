<% if (navbarType == 'plain') { %>
<%- include('navbarPlain'); %>
<% } else if (navbarType == 'student') { %>
<%- include('navbarStudent'); %>
<% } else if (navbarType == 'instructor') { %>
<%- include('navbarInstructor'); %>
<% } else if (navbarType == 'course') { %>
<%- include('navbarCourse'); %>
<% } else { %>
<% throw new Error('Unknown navbarType: ' + navbarType); %>
<% } %>
