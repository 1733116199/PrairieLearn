<% include('pointsFormatter'); %>

<% for (var i = 0; i < awarded_points.length; i++) { %>
    <% if (awarded_points[i] < init_points) { %>
        <% if (awarded_points[i] > 0) { %>
            <span class="label label-warning"><%= getStringFromFloat(awarded_points[i]) %></span>
        <% } %>
    <% } else { %>
        <span class="label label-success"><%= getStringFromFloat(awarded_points[i]) %></span>
    <% } %>
<% } %>
